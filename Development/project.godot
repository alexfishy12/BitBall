config_version=5

[application]

config/name="BitBall"
config/tags=PackedStringArray("2d")
run/main_scene="res://Scenes/MainMenu/main_menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/show_image=false
boot_splash/image="res://Scenes/Signature/RockyGamesFinished_640x360.png"
boot_splash/use_filter=false
config/icon="res://Entities/Ball/Ball_360x360.png"
config/windows_native_icon="res://Ball_360x360.ico"

[audio]

buses/default_bus_layout="res://configured_bus_layout.tres"

[autoload]

Singleton="*res://Autoloads/Singleton.tscn"
Events="*res://Autoloads/Events.tscn"
MusicManager="*res://Autoloads/Music/MusicManager.tscn"
SceneManager="*res://scene_manager.tscn"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/initial_screen=1
window/size/window_width_override=1920
window/size/window_height_override=1080
window/stretch/mode="viewport"

